[test_case]
test = perfrunner.tests.cbas.CBASBigfunDataSyncRebalanceTest

[showfast]
title = Sync data (BigFun 10K dataset) (data node rebalance in)
component = analytics
sub_category = BigFun 10K
category = SyncREB

[cluster]
mem_quota = 4096
analytics_mem_quota = 4096
initial_nodes = 2
num_buckets = 1

[bigfun]
workers=20
user_docs=5000

[rebalance]
nodes_after = 3
start_after = 60
stop_after = 60
