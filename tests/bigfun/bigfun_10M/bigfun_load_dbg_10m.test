[test_case]
test = perfrunner.tests.cbas.CBASBigfunTest
title = Load 10M dataset bigfun data into Couchbase for CBAS investigation
component = analytics
sub_category = BigFun 10M
category = Sync

[cluster]
mem_quota = 40960
analytics_mem_quota = 40960
analytics_iodevices=/data/@analytics/iodev0,/data/@analytics/iodev1,/data/@analytics/iodev2,/data/@analytics/iodev3,/data/@analytics/iodev4,/data/@analytics/iodev5,/data/@analytics/iodev6,/data/@analytics/iodev7
initial_nodes = 4
num_buckets = 1

[cbas_settings]
node_jvmArgs=-Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=9010 -Dcom.sun.management.jmxremote.local.only=false -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Xverify:none

[bigfun]
workers=40
user_docs=10000000

[access]
time = 60
