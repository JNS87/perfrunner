[test_case]
test = perfrunner.tests.analytics.BigFunSyncTest
use_workers = 0

[showfast]
component = analytics
category = sync
sub_category = BigFUN 20M
title = Initial sync time (min), 1 node, BigFUN 20M users (320M docs), 8 IO devices, 3 indexes

[cluster]
