[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest
title = Avg Throughput (ops/sec), Workload A, 3 nodes, throttled CPU
component = kv
category = ycsb

[cluster]
mem_quota = 40960
initial_nodes = 3
num_buckets = 1
