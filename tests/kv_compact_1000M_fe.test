[test_case]
test = perfrunner.tests.kv.CompactionTest
title = Full compaction time (min), 4 nodes, 1B x 1KB, 100K ops/sec (20/80 R/W), 5% cache miss ratio
component = kv
category = compact

[cluster]
mem_quota = 51200
initial_nodes = 4
num_buckets = 1

[bucket]
eviction_policy = fullEviction

[compaction]
db_percentage = 100

[load]
items = 1000000000
size = 1024
workers = 80

[hot_load]
items = 1000000000
workers = 100
working_set = 5

[access]
creates = 0
reads = 20
updates = 80
deletes = 0
items = 1000000000
working_set = 5
working_set_access = 95
workers = 40
throughput = 100000
async = 1
