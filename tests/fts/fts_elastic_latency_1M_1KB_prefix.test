[test_case]
test = perfrunner.tests.elastic.ElasticLatencyTest
title = PrefixQuery, wiki 1M x 1KB
summary = ELasticSearch latency test
larger_is_better = true

[fts]
items = 1000000
name  = perf_fts_index
port = 8094
worker= 10
type = prefix
query_file = perfrunner/tests/FTS/prefix.txt
query_size = 10
elastic = 1
logfile = /tmp/elastic_latency_prefix_1M_1K.txt
orderby = prefix

[access]
time = 1200

[cluster]
mem_quota = 51024
initial_nodes = 1
num_buckets = 1