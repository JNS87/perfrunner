[test_case]
test = perfrunner.tests.elastic.ElasticLatencyTest
title =  Match query , wiki 1M x 1KB
summary = elastic Latency test for match query
larger_is_better = true
fts = true


[fts]
items = 1000000
name  = perf_elastic_index
port = 9200
worker= 0
query = {"match": "dreamless", "field": "text"}
query_size = 10
elastic = 1
logfile = /tmp/elastic_latency_prefix_1M_1K.txt
orderby = match

[access]
time = 600


[cluster]
mem_quota = 51024
initial_nodes = 1
num_buckets = 1
